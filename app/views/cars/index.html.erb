 <div id="titulomarca"> 
    <h1>Actually Cars</h1>
 </div>

   
      
     <div id="atributos">
    
      <li class="atributon"> Image       </li>
      <li class="atributon"> Description </li>
      <li class="atributon"> Prize       </li>
      <li class="atributon"> Brand       </li>
    </div>
   
  <tbody>
    <% @cars.each do |car| %>
         <div class="cars_object">
         
          
    
        <%= image_tag(car.image ,:width => '300') %>

         <div class="descripcioncoche">  <%= car.Description %>      </div>
          <div class="atributocoche"> <%= car.km %>                 </div>
          <div class="atributocoche"> <%= car.brand.description%>   </div>
         <div class="acciones">  <%= link_to 'Show', car %>    </div>
          <div class="acciones"> <%= link_to 'Edit', car %>    </div>
           <div class="acciones"><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' }%> </div>
      
       </div>
    <% end %>
   
  </tbody>


<div id="crearmarca">
<% if !current_user.nil? %>
    <%= link_to 'New Brand', new_brand_path %>
    <%= link_to 'New Car', new_car_path %>
    </div>
<%end%>

<div id="paginate">
<%= will_paginate @brands %>
</div>
   